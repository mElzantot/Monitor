
@model List<EngineerSubTasks>

@for (int i = 0; i < Model.Count(); i++)
{
    <tr>
        <td>@Model[i].SubTask.Name</td>
        <td>@Model[i].SubTask.Description</td>
        <td>@Model[i].SubTask.Priority</td>
        <td>@Model[i].SubTask.Progress</td>
        <td>@Model[i].SubTask.EstDuration</td>
    </tr>
}
