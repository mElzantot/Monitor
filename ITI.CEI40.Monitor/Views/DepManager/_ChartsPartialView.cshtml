@model List<Activity>



@for (int i = 0; i < Model.Count; i++)
{
    <input class="taskname" id="n-@i" value="@Model[i].Name" hidden="hidden" />
    <input class="taskprog" id="p-@i" value="@Model[i].Progress" hidden="hidden" />
}


@section scripts{
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
}
